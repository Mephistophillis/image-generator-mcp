{
  "name": "openrouter-image-gen-mcp",
  "version": "1.0.0",
  "description": "MCP server for generating and editing images using OpenRouter's Nano Banana model",
  "main": "dist/index.js",
  "type": "module",
  "bin": {
    "openrouter-image-gen-mcp": "dist/index.js"
  },
  "scripts": {
    "build": "tsc && tsc-alias --resolve-full-paths",
    "dev": "tsc --watch",
    "start": "node dist/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write src/**/*.ts",
    "inspect": "npx @modelcontextprotocol/inspector dist/index.js",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint"
  },
  "keywords": [
    "mcp",
    "model-context-protocol",
    "openrouter",
    "image-generation",
    "nano-banana",
    "gemini",
    "ai",
    "typescript"
  ],
  "author": "OpenRouter Image Generator Team",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Mephistophillis/image-generator-mcp.git"
  },
  "bugs": {
    "url": "https://github.com/Mephistophillis/image-generator-mcp/issues"
  },
  "homepage": "https://github.com/Mephistophillis/image-generator-mcp#readme",
  "license": "MIT",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.20.2",
    "node-fetch": "^3.3.2",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/node": "^24.9.1",
    "@typescript-eslint/eslint-plugin": "^8.46.2",
    "@typescript-eslint/parser": "^8.46.2",
    "eslint": "^9.38.0",
    "jest": "^30.2.0",
    "jest-environment-node": "^30.2.0",
    "prettier": "^3.6.2",
    "ts-jest": "^29.4.5",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.16",
    "typescript": "^5.9.3"
  },
  "files": [
    "dist/**/*",
    "README.md",
    "LICENSE",
    "CHANGELOG.md",
    "server.json"
  ],
  "engines": {
    "node": ">=18.0.0"
  }
}
